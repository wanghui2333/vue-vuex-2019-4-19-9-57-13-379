;;;;;AAoBA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"CounterGroup.vue","sourceRoot":"src/components","sourcesContent":["<template>\n    <div class=\"counter-group\">\n        <h2>\n            Counter Number:\n            <span>{{counterNumber}}</span>\n        </h2>\n        <div class=\"counters\">\n            <Counter\n                    v-for=\"(counter,index) in $store.state.counters\"\n                    :index=\"index\"\n                    :value=\"counter.value\"\n                    :key=\"index\"\n                    @update=\"handleValueChange\"\n            />\n        </div>\n        <CounterSum></CounterSum>\n    </div>\n</template>\n\n<script>\n    import Counter from './Counter.vue'\n    import CounterSum from './CounterSum.vue'\n\n    export default {\n        name: 'counter-group',\n        components: {\n            Counter,\n            CounterSum\n        },\n        props: {\n            counterNumber: Number\n        },\n        created: function () {\n            // 根据 counterNumber 生成 counter 数据和组件\n            for (let i = 0; i < this.counterNumber; i++) {\n                this.$store.state.counters.push({\n                    value: 0\n                });\n            }\n        },\n        methods: {\n            handleValueChange: function (index, value,) {\n                this.$store.state.counters[index].value = value;\n            }\n        }\n    }\n</script>\n"]}]}